{
  "name": "se/sdk",
  "description": "SmartExpert SDK for Laravel 5.x, 6.x",
  "type": "library",
  "keywords": [
    "laravel",
    "se",
    "sdk"
  ],
  "require": {
    "php": "^7|^8",
    "bensampo/laravel-enum": "^4.2.0",
    "illuminate/support": "^5.5|^6",
    "guzzlehttp/guzzle": "^7",
    "webpatser/laravel-uuid": "^3.0",
    "spatie/image": "^1.4",
    "ext-curl": "*",
    "ext-json": "*",
    "laravel/framework": "6",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "SE\\SDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SE\\Tests\\SDK\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "laravel": {
      "providers": [
        "SE\\SDK\\ServiceProvider"
      ],
      "aliases": {
        "Tracking": "SE\\SDK\\Facades\\SeSDK"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
